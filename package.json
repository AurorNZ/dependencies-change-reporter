{
  "name": "report-updated-dependencies",
  "version": "1.0.0",
  "private": true,
  "description": "Github Action to adds a new comment to PR with a summary of changes to dependencies in a PR including release notes and changelogs",
  "main": "lib/main.js",
  "scripts": {
    "build": "rm -rf dist lib && tsc && ncc build --license licenses.txt --external re2 --external dtrace-provider",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "tscheck": " tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AurorNZ/report-updated-dependencies.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "AurorNZ",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@ungap/structured-clone": "^1.1.0",
    "bunyan": "1.8.15",
    "renovate": "35.66.3",
    "simple-git": "3.17.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/node": "16.x",
    "@vercel/ncc": "0.36.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.39.0",
    "eslint-plugin-github": "^4.7.0",
    "prettier": "2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
